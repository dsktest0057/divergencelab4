{
	"name": "ArchiveFolder",
	"properties": {
		"linkedServiceName": {
			"referenceName": "InvoicesBlob",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(utcnow(),'_',dataset().FileName)",
					"type": "Expression"
				},
				"container": "archive"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}